{
    "name": "richardpiel/cakephp-softdelete",
    "description": "Soft Delete Behavior for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/orm": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp": "^4.0",
        "phpunit/phpunit": "^8.5.0"
    },
    "autoload": {
        "psr-4": {
            "SoftDelete\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
